.list {
    padding-top: 20rpx;
}
.list .title {
    margin-bottom: 20rpx;
    padding: 10rpx 20rpx;
    font-size: 35rpx;
    border-bottom: 1px solid #eee;
}
.list .main {
    overflow: hidden;
    padding: 0 10rpx;
}
.list .item {
    float: left;
    margin-bottom: 10rpx;
    padding: 0 10rpx;
    width: 33.33333333%;
    box-sizing: border-box;
}

.list image {
    display: block;
    width: 100%;
    height: 250rpx;
}
.list text {
    display: block;
    padding: 10rpx 0;
    font-size: 28rpx;
    text-align: center;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}